{
  "name": "memoneo",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Alex Smith @nihiluis",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc -b",
    "build:sync": "npm run build && node ./bin/dev.js sync",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme"
  },
  "bin": {
    "cli": "./bin/run"
  },
  "homepage": "https://github.com/nihiluis/cliv2",
  "license": "MIT",
  "main": "dist/index.js",
  "dependencies": {
    "@inquirer/prompts": "^5.3.6",
    "@oclif/core": "^4.0.17",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@urql/core": "^5.0.5",
    "ansi-colors": "^4.1.3",
    "await-protect": "^1.0.17",
    "axios": "^1.7.2",
    "cli-progress": "^3.12.0",
    "dayjs": "^1.11.12",
    "detect-newline": "4.0.1",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "node-fetch": "^3.3.2",
    "node-fetch-commonjs": "^3.3.2",
    "rimraf": "^6.0.1",
    "uuid": "^10.0.0",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  },
  "oclif": {
    "bin": "memoneo",
    "dirname": "memoneo",
    "commands": "./dist/commands",
    "plugins": [],
    "topicSeparator": " "
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "types": "dist/index.d.ts"
}
